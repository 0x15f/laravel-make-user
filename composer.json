{
    "name": "dyrynda/laravel-make-user",
    "description": "An Artisan console command to create users in your Laravel application.",
    "license": "MIT",
    "keywords": ["laravel", "artisan", "console"],
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au",
            "homepage": "https://dyrynda.com.au"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.4",
        "illuminate/console": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/auth": "^5.4",
        "illuminate/notifications": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Dyrynda\\Artisan\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "^3.4",
        "orchestra/database": "^3.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Dyrynda\\Artisan\\MakeUserServiceProvider"
            ]
        }
    }
}
